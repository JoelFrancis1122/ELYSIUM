    <script>
        // Function to increase quantity
        function increaseQuantity(button) {
            var quantitySpan = button.previousElementSibling;
            var currentQuantity = parseInt(quantitySpan.textContent);
            quantitySpan.textContent = currentQuantity + 1;
        }
    
        // Function to decrease quantity
        function decreaseQuantity(button) {
            var quantitySpan = button.nextElementSibling;
            var currentQuantity = parseInt(quantitySpan.textContent);
            if (currentQuantity > 1) {
                quantitySpan.textContent = currentQuantity - 1;
            }
        }
    </script>


<script>
    function increase(pId) {
        fetch('/updateCart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                productId: pId,
                quantity: 1 // Increase by 1
            })
        })
        .then(response => {
            if (response.ok) {
                // Increment the quantity displayed on the page
                const quantityElement = document.getElementById('quantity_' + pId);
                // const quantityElement = document.getElementById('quantity_' + pId);
                quantityElement.textContent = parseInt(quantityElement.textContent) + 1;

                // Update the subtotal
                const subtotalElement = document.getElementById('subtotal_' + pId);
                const salePrice = document.getElementById('saleprice_' + pId);
                // console.log(salePrice * quantityElement.textContent)
                let saleP = parseFloat(salePrice.textContent.replace('$', ''));
                console.log(parseFloat(salePrice.textContent.replace('$', '')) )
                document.getElementById('subtotal_' + pId).innerText =saleP* parseInt(quantityElement.textContent);
                // const price = parseFloat(subtotalElement.textContent.replace('$', ''));
                // subtotalElement.textContent = '$' + (price + parseFloat(price / parseInt(quantityElement.textContent)));
            } else {
                console.error('Failed to update cart');
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }

    function decrease(pId) {
        fetch('/updateCart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                productId: pId,
                quantity: -1 // Decrease by 1
            })
        })
        .then(response => {
            if (response.ok) {
                // Decrement the quantity displayed on the page
                const quantityElement = document.getElementById('quantity_' + pId);
                const currentQuantity = parseInt(quantityElement.textContent);
                if (currentQuantity > 1) {
                    quantityElement.textContent = currentQuantity - 1;

                    // Update the subtotal
                    const subtotalElement = document.getElementById('subtotal_' + pId);
                    const price = parseFloat(subtotalElement.textContent.replace('$', ''));
                    subtotalElement.textContent = '$' + (price - parseFloat(price / currentQuantity));
                }
            } else {
                console.error('Failed to update cart');
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }
</script>
